int print2largest(int arr[], int n) {
	    int a=0,b=0;
	    for(int i=0;i<n;i++){
	        if(arr[i]>a){
	            a=arr[i];
	        }
	    }
	    for(int i=0;i<n;i++){
	        if(arr[i]>b && arr[i]!=a){
	            b=arr[i];
	        }
	    }
	    if(b>0){
	        return b;
	    }
	    else{
	        return -1;
	    }
